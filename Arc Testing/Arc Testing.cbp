<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Arc Testing" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="Project\;Scripts\;Resources\;" />
		<Build>
			<Target title="Debug">
				<Option output="Debug/ArcTesting" prefix_auto="1" extension_auto="1" />
				<Option object_output="Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-std=c++0x" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add directory="../Arc Engine/Arc Engine" />
				</Compiler>
				<Linker>
					<Add library="Arc" />
					<Add library="SDL" />
					<Add library="SDL_image" />
					<Add library="SDL_mixer" />
					<Add library="SDL_ttf" />
					<Add library="GL" />
					<Add library="GLU" />
					<Add library="lua" />
					<Add directory="../Arc Engine/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="Release/ArcTesting" prefix_auto="1" extension_auto="1" />
				<Option object_output="Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-std=c++0x" />
					<Add option="-Wall" />
					<Add option="-D_RELEASE" />
					<Add directory="../Arc Engine/Arc Engine" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="Arc" />
					<Add library="SDL" />
					<Add library="SDL_image" />
					<Add library="SDL_mixer" />
					<Add library="SDL_ttf" />
					<Add library="GL" />
					<Add library="GLU" />
					<Add library="lua" />
					<Add directory="../Arc Engine/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="Common.h">
			<Option virtualFolder="Project/" />
		</Unit>
		<Unit filename="Main.cpp">
			<Option virtualFolder="Project/" />
		</Unit>
		<Unit filename="TestApp.cpp">
			<Option virtualFolder="Project/" />
		</Unit>
		<Unit filename="TestApp.h">
			<Option virtualFolder="Project/" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
